                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_marginLeft="10dp"
                            >

                            <ImageView
                                android:id="@+id/image7"
                                android:layout_width="172dp"
                                android:layout_height="209dp"
