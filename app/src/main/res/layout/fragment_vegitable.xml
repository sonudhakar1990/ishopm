                            <TextView
                                android:layout_width="183dp"
                                android:layout_height="30dp"
                                android:text="Peas"
                                android:gravity="center"
                                android:textSize="18dp"
                                android:textColor="@color/btn_color"
                                />
                            <TextView
                                android:layout_width="183dp"
                                android:layout_height="30dp"
                                android:text="35/- Rs/KG"
                                android:gravity="center"
                                android:textSize="18dp"
                                />
                        </LinearLayout>
                    </RelativeLayout>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>

</FrameLayout>
